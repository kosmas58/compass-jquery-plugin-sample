            var jparent = $('#splunkjobs').parent();
            var sj = $('#splunkjobs');
            var offset = sj.offset();
            var height = sj.height();
            var width = sj.width();
            var abscopy = sj.
                clone(false).
                attr('id', 'jobspopup').
                addClass('splunkjobs').
                css('position', 'absolute').
                css('backgroundColor', 'white').
                css('zIndex', 1000).
                offset(offset).
                width(width).
                height(height).
                appendTo(jparent).
                mouseleave(function() {
                    $('#shade').
                        animate({
                            opacity: 0
                        }, 1000, function() { $(this).remove(); });
                    $(this).animate({
                        width: '-=300px',
                        height: height,
                        left: '+=300px'
                    }, 1000, function() { $(this).remove(); });
                    $('#splunkjobsdetail').animate({
                        'opacity': 0
                        }, 1000, function() { $(this).remove(); });
                    //$(this).remove();
                }).
                animate({
                    width: '+=300px',
                    height: '350px',
