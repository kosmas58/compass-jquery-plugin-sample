-@title = "jQuery Mobile Docs - List Icons"
= render :partial => "/mobile/index.js.haml", :format => :js

.type-interior{"data-role" => "page"}
  %div{"data-role" => "header", "data-theme" => "f"}
    %h1 Icons
    %a.ui-btn-right.jqm-home{"data-direction" => "reverse", "data-icon" => "home", "data-iconpos" => "notext", :href => "/mobile"} Home
  / /header
  %div{"data-role" => "content"}
    .content-primary
      %ul{"data-role" => "listview"}
        %li
          %a{:href => "/mobile/docs/lists"}
            %img.ui-li-icon{:alt => "France", :src => "/images/demo/mobile/flags/gf.png"}
              France
              %span.ui-li-count 4
        %li
          %a{:href => "/mobile/docs/lists"}
            %img.ui-li-icon{:alt => "Germany", :src => "/images/demo/mobile/flags/de.png"}
              Germany
              %span.ui-li-count 4
        %li
          %a{:href => "/mobile/docs/lists"}
            %img.ui-li-icon{:alt => "Great Britain", :src => "/images/demo/mobile/flags/gb.png"}
              Great Britain
              %span.ui-li-count 0
        %li
          %a{:href => "/mobile/docs/lists"}
            %img.ui-li-icon{:alt => "Finland", :src => "/images/demo/mobile/flags/fi.png"}
              Finland
              %span.ui-li-count 12
        %li
          %a{:href => "/mobile/docs/lists"}
            %img.ui-li-icon{:alt => "Norway", :src => "/images/demo/mobile/flags/sj.png"}
              Norway
              %span.ui-li-count 328
        %li
          %a{:href => "/mobile/docs/lists"}
            %img.ui-li-icon{:alt => "United States", :src => "/images/demo/mobile/flags/us.png"}
              United States
              %span.ui-li-count 62
    / /content-primary
    .content-secondary
      .ui-body-b
        %div{"data-collapsed" => "true", "data-content-theme" => "d", "data-role" => "collapsible", "data-theme" => "b"}
          %h3 More in this section
          %ul{"data-dividertheme" => "d", "data-role" => "listview", "data-theme" => "c"}
            %li{"data-role" => "list-divider"} List views
            %li
              %a{:href => "/mobile/docs/lists/docs_lists"} List markup conventions
            %li
              %a{:href => "/mobile/docs/lists/ul"} Basic linked list
            %li
              %a{:href => "/mobile/docs/lists/nested"} Nested list
            %li
              %a{:href => "/mobile/docs/lists/ol"} Numbered list
            %li
              %a{:href => "/mobile/docs/lists/split"} Split button list
            %li
              %a{:href => "/mobile/docs/lists/divider"} List dividers
            %li
              %a{:href => "/mobile/docs/lists/count"} Count bubble
            %li
              %a{:href => "/mobile/docs/lists/thumbnails"} Thumbnails
            %li{"data-theme" => "a"}
              %a{:href => "/mobile/docs/lists/icons"} Icons
            %li
              %a{:href => "/mobile/docs/lists/formatting"} Content formatting
            %li
              %a{:href => "/mobile/docs/lists/search"} Search filter bar
            %li
              %a{:href => "/mobile/docs/lists/search_inset"} Inset search filter bar
            %li
              %a{:href => "/mobile/docs/lists/search_with_dividers"} Search filter bar with dividers
            %li
              %a{:href => "/mobile/docs/lists/readonly"} Read-only lists
            %li
              %a{:href => "/mobile/docs/lists/readonly_inset"} Read-only inset lists
            %li
              %a{:href => "/mobile/docs/lists/forms"} Lists with forms
            %li
              %a{:href => "/mobile/docs/lists/forms_inset"} Inset lists with forms
            %li
              %a{:href => "/mobile/docs/lists/inset"} Inset styled lists
            %li
              %a{:href => "/mobile/docs/lists/performance"} List performance test
            %li
              %a{:href => "/mobile/docs/lists/themes"} Theming lists
  / /content
  .footer-docs{"data-role" => "footer", "data-theme" => "c"}
    %p &copy; 2011 The jQuery Project
/ /page
