-@title = "#{t('txt.jqgrid.name')} #{t('txt.jqgrid.demo.name')} - #{t('txt.jqgrid.demo.30live')} - #{t('txt.jqgrid.demo.30navigator')}"
.demo
  %h3
    =image_tag("icons/22x22/grid.png", :border => 0)
    =@title
  %center
    %table#invheaders_demo0805.scroll{ :cellspacing => "0", :cellpadding => "0" }
    #invheaders_demo0805_pager.scroll{ :style => "text-align:center;" }

/ End demo 
.demo-description
  %p
    This example shows the navigator.
    See all available options below.
  %p
    This method uses
    %i
      colModel
    names and
    %i
      editurl
    parameters from jqGrid
  %p
    Calling:
    %code
      jQuery("#grid_id").jqGrid('navGrid',selector,options,pEdit,pAdd,pDel,pSearch );
/ End demo-description
